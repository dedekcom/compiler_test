func fibo(first, second, n):
    if n <= 0:
        if (n * (10-5/2)) == 0 && ( 1 != n || (1 >= 2 && 2 < 2)):
            println("parsed long if")
            if first > 0 && second > 0:
                if first < 0:
                else:
                    println("parsed nested else")
                endif
            endif
        endif
        second
    else:
        println("n=" + n + " is " + second)
        fibo(second, first+second, n-1)
    endif
end

func main():
    val f = fibo((1), (((1+0))), 5)
    println("fibo for 5 is " + f)
end
